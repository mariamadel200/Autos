@model Autos.Models.VehicleForSale

<div class="text-center wow fadeInUp" data-wow-delay="0.1s">
    <h1 class="section-title text-center text-primary text-uppercase">Period For Rental</h1>

</div>
<form asp-controller="Rental" asp-action="Period" method="post">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <label>StartDate:</label>
        <input name="start" type="date" id="datepicker" class="form-control" />
       
    </div>
    <div class="form-group">
        <label>EndDate:</label>
        <input name="end" type="date" id="datepicker" class="form-control" />

    </div>
    <button type="submit" class="btn btn-primary">Done</button>
</form>
