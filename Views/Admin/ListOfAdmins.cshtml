@model List<User>
<div class="text-center wow fadeInUp" data-wow-delay="0.1s">
    <h1 class="section-title text-center text-primary text-uppercase">Admins</h1>
    <br></br>
</div>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var u in Model)
        {
            <tr>
                <td>@u.Firstname @u.Lastname</td>
                <td>@u.Email</td>
                <td>

                    <form asp-controller="Admin" asp-action="RemoveAdmin" asp-route-id="@u.Id">
                        <button type="submit" class="btn btn-danger">Remove</button>
                    </form>
                </td>
            </tr>
        }
    </tbody>
</table>

<div>
    <a asp-area="Identity" asp-page="/Account/Register" class="btn btn-success">Add Admin</a>
</div>
